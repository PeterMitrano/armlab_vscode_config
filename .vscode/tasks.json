{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "my catkin make",
            "type": "shell",
            "command": "catkin_make -DCMAKE_EXPORT_COMPILE_COMMANDS=1 --cmake-args -DCMAKE_BUILD_TYPE=Release -DCATKIN_ENABLE_TESTING=OFF",
            "problemMatcher": {
                "base": "$gcc",
                "fileLocation": [
                    "absolute"
                ]
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "classifiers tensorboard",
            "type": "shell",
            "command": "tensorboard --logdir trials",
            "problemMatcher": [],
            "options": {
                "cwd": "${workspaceFolder}/src/link_bot/link_bot_classifiers"
            }
        }
    ]
}